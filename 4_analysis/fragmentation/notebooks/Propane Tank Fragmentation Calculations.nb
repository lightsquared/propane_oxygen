  3.624290765099107*^9}, {3.6242908383848877`*^9, 3.624290852943303*^9}, {
  3.624290933752489*^9, 3.624290941085883*^9}, {3.6242912232014227`*^9, 
  3.624291229544591*^9}, {3.624291278615574*^9, 3.62429131607847*^9}, {
  3.624291376059409*^9, 3.6242914125822153`*^9}, {3.624299045597979*^9, 
